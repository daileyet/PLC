#amap-container {
	width: 100%;
	min-height: 600px;
	margin: 0px;
	padding: 0px;
}

.plc-amap-panel-expand {
	position: relative;
	margin: 0px;
	margin-top: 10px;
	width: 100%;
	height: 100%;
}

.amap-marker .marker-route{
	position: absolute;
	width: 32px;
	height: 32px;
	color: #e90000;
	cursor: pointer;
}

.amap-marker .marker-0 {
	background: url(../../static/img/marker_0.png) no-repeat;
}

.amap-marker .marker-1 {
	background: url(../../static/img/marker_1.png) no-repeat;
}

.amap-marker .marker-5 {
	background: url(../../static/img/marker_5.png) no-repeat;
}

.amap-marker .marker-8 {
	background: url(../../static/img/marker_8.png) no-repeat;
}


.form-base .form-group{
	padding: 0;
	margin-bottom: 5px;
}
